{
  "name": "ammobin-classifier",
  "version": "0.0.31",
  "description": "",
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "scripts": {
    "test:lint": "tslint --project ./tsconfig.json './src/**/*.ts' ",
    "test:unit": "jest --env=node",
    "test": "npm run test:lint && npm run test:unit",
    "build": "tsc",
    "prepublish": "npm run build",
    "format": "pretty-quick"
  },
  "author": "nexus-uw",
  "license": "MIT",
  "dependencies": {
    "natural": "^0.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@types/jest": "^24.0.5",
    "@types/natural": "^0.2.32",
    "husky": "^1.3.1",
    "jest": "^22.0.0",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "ts-jest": "^22.0.1",
    "tslint": "5.9.0",
    "typescript": "^2.9.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/?(*.)(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testEnvironment": "node"
  }
}
