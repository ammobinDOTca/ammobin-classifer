{
  "name": "ammobin-classifier",
  "version": "0.0.26",
  "description": "",
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "scripts": {
    "test:lint": "tslint --type-check --project ./tsconfig.json './src/**/*.ts' ",
    "test:unit": "jest",
    "test": "npm run test:lint && npm run test:unit",
    "build": "tsc",
    "prepublish": "npm run build"
  },
  "author": "nexus-uw",
  "license": "MIT",
  "dependencies": {
    "natural": "^0.5.6"
  },
  "devDependencies": {
    "@types/jest": "^22.0.0",
    "@types/natural": "^0.2.32",
    "jest": "^22.0.0",
    "ts-jest": "^22.0.1",
    "tslint": "5.9.0",
    "typescript": "^2.3.4"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/?(*.)(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testEnvironment": "node"
  }
}
